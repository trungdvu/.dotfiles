{
  "breadcrumbs.enabled": false,
  "css.lint.unknownAtRules": "ignore",
  "css.validate": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": { "strings": true },
  "editor.renderControlCharacters": false,
  "editor.tabSize": 2,
  "editor.linkedEditing": true,
  "security.workspace.trust.untrustedFiles": "open",
  "window.newWindowDimensions": "inherit",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Default Dark+ Experimental",
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-a>": false
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["v", "n"],
      "after": ["v", "g", "_", "y"]
    },
    {
      "before": ["d", "n"],
      "after": ["d", "g", "_"]
    },
    {
      "before": ["<C-d>"],
      "after": ["C-d", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["C-u", "z", "z"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    },
    {
      "before": ["g", "d"],
      "commands": [{ "command": "editor.action.goToTypeDefinition" }]
    },
    {
      "before": ["g", "i"],
      "commands": [{ "command": "editor.action.goToImplementation" }]
    },
    {
      "before": ["K"],
      "commands": [{ "command": "editor.action.showHover" }]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [{ "command": "editor.action.formatDocument" }]
    },
    {
      "before": ["<leader>", "o"],
      "commands": [{ "command": "editor.action.organizeImports" }]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": [{ "command": "editor.action.rename" }]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["p"],
      "commands": ["editor.action.clipboardPasteAction"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["turboConsoleLog.displayLogMessage"]
    }
  ],
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 100,
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false
}
